{
  "id": "zkui.json",
  "cmd": null,
  "cpus": 0.1,
  "mem": 512,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "maauso/zkui:latest",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 9090,
          "hostPort": 0,
          "servicePort": 10102,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "env": {
    "ZKLIST": "IP"
  },
  "healthChecks": [
    {
      "protocol": "TCP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
}
