{
  "id": "/test/nginx",
  "cmd": null,
  "cpus": 0.1,
  "mem": 512,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "nginx",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "protocol": "tcp",
          "name": "nginx",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
    "SERVICE_80_NAME": "miguel-nginx",
    "SERVICE_80_TAGS": "miguel2",
    "SERVICE_80_CHECK_INTERVAL":"15s",
    "SERVICE_80_CHECK_TCP": "true",
    "SERVICE_80_CHECK_TIMEOUT": "5s"
  },
  "healthChecks": [
    {
      "protocol": "TCP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ]
}
